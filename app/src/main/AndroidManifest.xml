<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sy_studio.the.gugudan_SangYeol">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/gugudan_icon_1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gugudan_icon_1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sy_studio.the.gugudan_SangYeol.MainActivity" />
        <activity android:name="com.sy_studio.the.gugudan_SangYeol.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.sy_studio.the.gugudan_SangYeol.WriteActivity" />
        <activity
            android:name="com.sy_studio.the.gugudan_SangYeol.LoginActivity"
            android:theme="@style/hideTitleDialog" />
        <activity
            android:name="com.sy_studio.the.gugudan_SangYeol.SignUpActivity"
            android:theme="@style/hideTitleDialog" />
        <activity android:name="com.sy_studio.the.gugudan_SangYeol.SignActivity" />
    </application>

</manifest>