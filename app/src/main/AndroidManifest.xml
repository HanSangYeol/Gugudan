<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sy_studio.the.gugudan_SangYeol">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/gugudan_icon_1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gugudan_icon_1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data android:name="com.timeLine.sdk.ApplicationId"
            android:value="@string/facebook_app_id"/>

        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WriteActivity" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/hideTitleDialog" />
        <activity
            android:name=".SignUpActivity"
            android:theme="@style/hideTitleDialog" />
        <activity android:name=".SignActivity" />
        <activity android:name=".GalleryActivity" />
        <activity android:name=".YoutubeActivity"></activity>
    </application>

</manifest>